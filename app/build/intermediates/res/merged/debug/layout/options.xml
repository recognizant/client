<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_launcher"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.google.android.gms.samples.vision.ocrreader.LauncherActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/camera"
            android:id="@+id/imgVwCamera"
            android:clickable="true"
            android:layout_marginLeft="60dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtVwCamera"
            android:text="Camera"
            android:layout_below="@id/imgVwCamera"
            android:layout_marginLeft="90dp"
            android:textSize="15dp"/>


        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/gallery"
            android:id="@+id/imgVwGallery"
            android:clickable="true"
            android:layout_toRightOf="@id/imgVwCamera"
            android:layout_marginLeft="30dp"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtVwGallery"
            android:text="Gallery"
            android:layout_toRightOf="@id/txtVwCamera"
            android:layout_below="@id/imgVwCamera"
            android:layout_marginLeft="75dp"
            android:textSize="15dp"/>



        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/cities"
            android:id="@+id/imgVwCities"
            android:layout_below="@id/txtVwCamera"
            android:clickable="true"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="60dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtVwCities"
            android:text="Cities"
            android:layout_below="@id/imgVwCities"
            android:layout_marginLeft="90dp"
            android:textSize="15dp"/>


        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/about"
            android:id="@+id/imgVwAbout"
            android:clickable="true"
            android:layout_marginTop="40dp"
            android:layout_below="@id/txtVwGallery"
            android:layout_toRightOf="@id/imgVwCamera"
            android:layout_marginLeft="30dp"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtVwAboutUs"
            android:text="About Us"
            android:layout_toRightOf="@id/txtVwCamera"
            android:layout_below="@id/imgVwAbout"
            android:layout_marginLeft="70dp"
            android:textSize="15dp"/>



        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/help"
            android:id="@+id/imgVwHelp"
            android:layout_below="@id/txtVwCities"
            android:clickable="true"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="60dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtVwHelp"
            android:text="Help"
            android:layout_below="@id/imgVwHelp"
            android:layout_marginLeft="90dp"
            android:textSize="15dp"/>


        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/rate"
            android:id="@+id/imgVwRating"
            android:clickable="true"
            android:layout_marginTop="40dp"
            android:layout_below="@id/txtVwAboutUs"
            android:layout_toRightOf="@id/imgVwCamera"
            android:layout_marginLeft="30dp"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/txtVwRatings"
            android:text="Ratings"
            android:layout_toRightOf="@id/txtVwHelp"
            android:layout_below="@id/imgVwHelp"
            android:layout_marginLeft="90dp"
            android:textSize="15dp"/>



    </RelativeLayout>

</RelativeLayout>
